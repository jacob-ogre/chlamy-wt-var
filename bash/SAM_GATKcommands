cd $PICARD && java -Xmx1G -jar AddOrReplaceReadGroups.jar INPUT=/scratch/01703/jmalcom/Chlamy_RAD_SAM/Ch_1373_fq.sam OUTPUT=$UPDAT/Ch_1373_updated.sam RGID="0" RGLB="Aug12" RGPL="solid" RGPU="BC0" RGSM="1373" && cd $SAMTL && samtools view -b -q 30 -S $UPDAT/Ch_1373_updated.sam  > $BAMS/1373.bam && samtools sort $BAMS/1373.bam $SORT/1373_sorted && samtools index $SORT/1373_sorted.bam && samtools mpileup -uEf $CRREF/Cre_comp_ref.fa $SORT/1373_sorted.bam > $BCFS/1373.bcf && bcftools/bcftools view -cg $BCFS/1373.bcf > $VCFS/1373_SAM.vcf && cd $WORK/GATK && pwd && java -Xmx1G -jar GenomeAnalysisTK.jar -T RealignerTargetCreator  -I $SORT/1373_sorted.bam  -R $CRREF/Cre_comp_ref.fa  -o $REAL/1373_realigner.intervals && java -Xmx1G -jar GenomeAnalysisTK.jar -T IndelRealigner -I $SORT/1373_sorted.bam -R $CRREF/Cre_comp_ref.fa -targetIntervals $REAL/1373_realigner.intervals -o $RBAM/1373_realigned.bam && java -Xmx1G -jar GenomeAnalysisTK.jar -T UnifiedGenotyper -nt 12 -R $CRREF/Cre_comp_ref.fa  -I $RBAM/1373_realigned.bam  -ploidy $PLOIDY -glm BOTH -out_mode EMIT_ALL_CONFIDENT_SITES -o $VCFS/1373_GATK.vcf && python $VCFS/VCFHighQualityVariants.py $VCFS/1373_GATK.vcf $SNPS && java -Xmx1G -jar GenomeAnalysisTK.jar -T BaseRecalibrator -I $RBAM/1373_realigned.bam -R $CRREF/Cre_comp_ref.fa -knownSites $SNPS/1373_HQ_SNPs.vcf -o $RECAL/1373_recal_data.grp && java -Xmx1G -jar GenomeAnalysisTK.jar -T PrintReads -I $RBAM/1373_realigned.bam -R $CRREF/Cre_comp_ref.fa -BQSR $RECAL/1373_recal_data.grp -o $REREBAM/1373_realigned_recal.bam && java -Xmx1G -jar GenomeAnalysisTK.jar -T UnifiedGenotyper -nt 12 -R $CRREF/Cre_comp_ref.fa -I $REREBAM/1373_realigned_recal.bam -ploidy $PLOIDY -glm BOTH -out_mode EMIT_ALL_CONFIDENT_SITES -o $VCFS/1373_GATK_recal.vcf && python $VCFS/FilterVCFHaploid.py 30 $VCFS/1373_SAM.vcf $VCFS/filtered_vcfs ## End of commands for sample 1373
